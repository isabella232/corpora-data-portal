{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@blueprintjs/core": "^3.34.1",
    "@loadable/component": "^5.14.0",
    "babel-plugin-styled-components": "^1.11.1",
    "bl": "^4.0.3",
    "clipboard-copy": "^3.1.0",
    "gatsby": "^2.24.83",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-offline": "^3.2.35",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typescript": "^2.4.23",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-sharp": "^2.5.19",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.2.0",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@blueprintjs/eslint-plugin": "^0.2.2",
    "@types/jest": "^26.0.15",
    "@types/loadable__component": "^5.13.1",
    "@types/reach__router": "^1.3.6",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "@types/theme-ui": "^0.3.7",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "babel-preset-gatsby": "^0.5.14",
    "concurrently": "^5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-playwright": "^0.2.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "expect-playwright": "^0.2.5",
    "gatsby-plugin-asset-path": "^3.0.2",
    "gatsby-plugin-root-import": "^2.0.5",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "jest-playwright-preset": "^1.3.1",
    "lint-staged": "^10.4.2",
    "playwright": "^1.5.1",
    "prettier": "2.1.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "start-server-and-test": "^1.11.5",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "npx prettier --write .",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest jest/config.js",
    "e2e": "TEST_ENV=local jest --config jest/e2e.config.js",
    "e2e-localProd": "TEST_ENV=localProd jest --config jest/e2e.config.js",
    "e2e-localProd-ci": "DEBUG=pw:api TEST_ENV=localProd jest --config jest/e2e.config.js",
    "e2e-dev": "TEST_ENV=dev jest --config jest/e2e.config.js",
    "e2e-staging": "TEST_ENV=staging jest --config jest/e2e.config.js",
    "e2e-prod": "TEST_ENV=prod jest --config jest/e2e.config.js",
    "type-check": "tsc --noEmit",
    "lint": "concurrently \"node_modules/.bin/eslint src/**/*.{ts,tsx} --quiet --fix\" \"node_modules/.bin/stylelint --fix '**/*.{js,ts,tsx,css}'\"  \"npm run type-check\"",
    "prettier-check": "npx prettier --check .",
    "build-and-start-prod": "npm run build && npm run serve",
    "_build-and-start-local-backend": "make local-database -C ../ && make local-backend -C ../",
    "build-and-start-local-backend": "DEPLOYMENT_STAGE=test AWS_PROFILE=single-cell-dev npm run _build-and-start-local-backend",
    "build-and-start-local-backend-ci": "make local-database -C ../ && make local-backend -C ../",
    "smoke-test": "start-server-and-test start :8000 e2e",
    "smoke-test-prod-build": "start-server-and-test build-and-start-prod :9000 e2e-localProd",
    "smoke-test-with-local-backend": "start-server-and-test build-and-start-local-backend :5000 build-and-start-prod :9000 e2e-localProd",
    "smoke-test-with-local-backend-ci": "start-server-and-test build-and-start-local-backend-ci :5000 build-and-start-prod :9000 e2e-localProd-ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
